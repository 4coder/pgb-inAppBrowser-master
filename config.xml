<?xml version="1.0" encoding="UTF-8"?> 
<widget xmlns="http://www.w3.org/ns/widgets" 
		xmlns:gap="http://phonegap.com/ns/1.0"
		id="com.inappbrowser.pgbtest"
		version="1.1.1">
	
	<name>
		Edgewater Inc.
	</name>
	<description>
		Edgewater IOS Application
	</description>

	<author href="" email="info@sharpedgelearning.com">
		SHArP Edge
	</author>

	<!-- @note gap:platforms not working in PGB! -->
	<gap:platform name="ios" />
	<gap:platform name="android" minVersion="2.1"/>

	<gap:plugin name="org.apache.cordova.inappbrowser" />

	<!-- PG version -->
	<preference name="phonegap-version" value="3.1.0" />
	<!--
		We don't really need specific permissions,
		but PGB will add some.
	-->
	<preference name="permissions" value="none"/>

	<access origin="*" />

</widget>
